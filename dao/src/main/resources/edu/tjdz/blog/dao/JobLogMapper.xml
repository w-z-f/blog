<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="edu.tjdz.blog.dao.JobLogMapper">

    <insert id="save" parameterType="edu.tjdz.blog.beans.bean.JobLog">
        insert into job_log values(#{id},#{beanName},#{jobId},#{params},#{status},#{error},#{times},#{createTime});
    </insert>


</mapper>